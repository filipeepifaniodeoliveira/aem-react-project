<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Not found Container"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <items jcr:primaryType="nt:unstructured">
         <tabs jcr:primaryType="nt:unstructured"
               sling:resourceType="granite/ui/components/coral/foundation/tabs">
            <items jcr:primaryType="nt:unstructured">
               <tab-1 jcr:primaryType="nt:unstructured"
                      jcr:title="Image"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <imageContainerResType jcr:primaryType="nt:unstructured"
                                                  name="./imageContainer/sling:resourceType"
                                                  sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                  value="core/wcm/components/image/v2/image"/>
                           <imageContainer allowUpload="{Boolean}false"
                                           autoStart="{Boolean}false"
                                           class="cq-droptarget"
                                           cq-msm-lockable="./imageContainer/file"
                                           fieldLabel="Imagem"
                                           fileNameParameter="./imageContainer/fileName"
                                           fileReferenceParameter="./imageContainer/fileReference"
                                           jcr:primaryType="nt:unstructured"
                                           mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                           multiple="{Boolean}false"
                                           name="./imageContainer/file"
                                           sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                           title="Drag to select image"
                                           uploadUrl="${suffix.path}"
                                           useHTML5="{Boolean}true"/>
                        </items>
                     </column>
                  </items>
               </tab-1>
               <tab-2 jcr:primaryType="nt:unstructured"
                      jcr:title="Texts"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <titleNotFound cq-msm-lockable="./titleNotFound"
                                          fieldLabel="Digite o seu título."
                                          jcr:primaryType="nt:unstructured"
                                          name="./titleNotFound"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <descriptNotFound cq-msm-lockable="./descriptNotFound"
                                             fieldLabel="Digite o seu texto."
                                             jcr:primaryType="nt:unstructured"
                                             name="./descriptNotFound"
                                             sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                        </items>
                     </column>
                  </items>
               </tab-2>
               <tab-3 jcr:primaryType="nt:unstructured"
                      jcr:title="Button"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <buttonNotFound cq-msm-lockable="./buttonNotFound"
                                           fieldLabel="Digite o Placeholder do Botão."
                                           jcr:primaryType="nt:unstructured"
                                           name="./buttonNotFound"
                                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                        </items>
                     </column>
                  </items>
               </tab-3>
            </items>
         </tabs>
      </items>
   </content>
</jcr:root>
